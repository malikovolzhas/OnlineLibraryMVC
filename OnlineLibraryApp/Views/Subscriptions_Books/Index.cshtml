@model IEnumerable<OnlineLibraryApp.Models.Subscriptions_Books>

@{
    ViewBag.Title = "Subscription List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Book.name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Subscription.last_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subscription.first_name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Book.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subscription.last_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subscription.first_name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
